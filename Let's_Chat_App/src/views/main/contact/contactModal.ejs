<div class="modal fade" id="contactsModal" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Quản lý liên lạc</h4>
                </div>
                <div class="modal-body">
                    <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="#find-user">Tìm người dùng</a></li>
                        <li><a data-toggle="tab" id="link-contacts" href="#contacts">
                            Danh bạ 
                            <% if(countAllContacts > 0){ %>
                                <span class="show-number-contacts count-contacts">
                                    (`<em><%= countAllContacts %></em>`)
                                </span>
                            <%}else{ %>
                                <span class="show-number-contacts count-contacts display-none">
                                    </span>
                                
                            <%} %>
                            
                        </a></li>
                        <li><a data-toggle="tab" id="link-request-contact-sent" href="#request-contact-sent">
                            Đang chờ xác nhận 
                            <% if( countAllContactsSent > 0){ %>
                                <span class="show-number-contacts count-request-contact-sent">
                                    (`<em><%= countAllContactsSent %></em>`)
                                </span>
                            <%}else{ %>
                                <span class="show-number-contacts count-request-contact-sent display-none"></span>
                            <%} %>
                            
                        </a></li>
                        <li><a data-toggle="tab" id="link-request-contact-received" href="#request-contact-received">
                            Yêu cầu kết bạn 
                            <% if(countAllContactsReviece > 0){ %>
                                <span class="show-number-contacts count-request-contact-received">
                                    (`<em><%= countAllContactsReviece %></em>`)
                                </span>
                            <%}else{ %>
                                <span class="show-number-contacts count-request-contact-received display-none"></span>
                                    
                            <%} %>
                            
                        </a></li>
                    </ul>
                    <div class="tab-content">
                        <div id="find-user" class="tab-pane fade in active">
                            <div class="find-user-top">
                                <div class="input-group col-md-12">
                                    <input id="input-find-users-contact" type="text" class="form-control" placeholder="Nhập E-mail hoặc username..." />
                                    <span class="input-group-btn">
                                        <button id="btn-find-users-contact" class="btn btn-lg" type="button">
                                            <i class="glyphicon glyphicon-search"></i>
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <div class="find-user-bottom">
                                <ul class="contactList">
        
                                </ul>
                            </div>
                        </div>
                        <div id="contacts" class="tab-pane fade">
                            <div class="find-user-bottom">
                                <ul class="contactList">
                                    <% contacts.forEach(function(user) {  %> 
                                        <li class="_contactList" data-uid="<%= user._id %>">
                                            <div class="contactPanel">
                                                <div class="user-avatar">
                                                    <img src="../../images/users/<%= user.avatar %>" alt="">
                                                </div>
                                                <div class="user-name">
                                                    <p>
                                                        <%= user.username %>
                                                    </p>
                                                </div>
                                                <br>
                                                <div class="user-address">
                                                    <span>&nbsp <%= user.address %></span>
                                                </div>
                                                <div class="user-talk" data-uid="<%= user._id %>">
                                                    Trò chuyện
                                                </div>
                                                <div class="user-remove-contact action-danger" data-uid="<%= user._id %>">
                                                    Xóa liên hệ
                                                </div>
                                            </div>
                                        </li>
                                    <% }); %>
                                </ul>
                                <hr>
                                    <div class="read-more-contacts">
                                        <a href="#" id="read-more-contacts">
                                                <strong>Xem thêm 10 bạn bè</strong>
                                        </a>
                                    </div>
                                <hr>
                            </div>
                        </div>
                        <div id="request-contact-sent" class="tab-pane fade">
                            <div class="find-user-bottom">
                                <ul class="contactList">
                                    <% contactsSent.forEach(function(user) {  %> 
                                        <li class="_contactList" data-uid="<%= user._id %>">
                                                <div class="contactPanel">
                                                    <div class="user-avatar">
                                                        <img src="../../images/users/<%= user.avatar %>" alt="">
                                                    </div>
                                                    <div class="user-name">
                                                        <p>
                                                            <%= user.username %>
                                                        </p>
                                                    </div>
                                                    <br>
                                                    <div class="user-address">
                                                        <span>&nbsp <%= user.address %></span>
                                                    </div>
                                                    <div class="user-remove-request-contact-sent action-danger display-important" data-uid="<%= user._id %>">
                                                        Hủy yêu cầu
                                                    </div>
                                                </div>
                                            </li>
                                    <% }); %>
                                    
                                </ul>
                                <hr>
                                <div class="read-more-contacts-sent">
                                    <a href="#" id="read-more-contacts-sent">
                                            <strong>Xem thêm 10 người khác</strong>
                                    </a>
                                </div>
                                <hr>
                            </div>
                        </div>
                        <div id="request-contact-received" class="tab-pane fade">
                            <div class="find-user-bottom">
                                <ul class="contactList">
                                    <% contactsReviece.forEach(function(user) {  %> 
                                        <li class="_contactList" data-uid="<%= user._id %>">
                                                <div class="contactPanel">
                                                    <div class="user-avatar">
                                                        <img src="../../images/users/<%= user.avatar %>" alt="">
                                                    </div>
                                                    <div class="user-name">
                                                        <p>
                                                                <%= user.username %>
                                                        </p>
                                                    </div>
                                                    <br>
                                                    <div class="user-address">
                                                        <span>&nbsp <%= user.address %></span>
                                                    </div>
                                                    <div class="user-acccept-contact-received" data-uid="<%= user._id %>">
                                                        Chấp nhận
                                                    </div>
                                                    <div class="user-reject-request-contact-received action-danger" data-uid="user._id">
                                                        Xóa yêu cầu
                                                    </div>
                                                </div>
                                            </li>
                                    
                                    <% }); %>
                                    
                                </ul>
                                <hr>
                                <div class="read-more-contacts">
                                    <a href="#" id="read-more-contacts-reviece">
                                            <strong>Xem thêm 10 người khác</strong>
                                    </a>
                                </div>
                                <hr>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>